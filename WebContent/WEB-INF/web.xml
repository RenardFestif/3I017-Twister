<?xml  version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app  PUBLIC
" -//Sun  Microsystems , Inc.//DTD  Web  Application  2.3//EN" 
"http :// java.sun.com/dtd/web -app_2_3.dtd">

<web - app>

	<!-- Servlets -->
	<servlet>
		<servlet-name>SignIn</servlet-name>
		<servlet-class>CreateUser</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>LogIn</servlet-name>
		<servlet-class>LogUser</servlet-class>
	</servlet>


	<!-- Mapping -->
	<servlet-mapping>
		<servlet-name>SignIn</servlet-name>
		<url-pattern>/Twister/Acceuil/signin </url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>LogIn</servlet-name>
		<url-pattern>/Twister/Acceuil/login </url-pattern>
	</servlet-mapping>
	
</web-app>